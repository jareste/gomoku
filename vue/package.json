{
  "name": "vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "wasm": "cd ../rust; wasm-pack build . --target web; cd ../vue",
    "all": "cd ../rust; wasm-pack build . --target web; cd ../vue; vite build; vite preview"
  },
  "dependencies": {
    "@wasm-tool/wasm-pack-plugin": "^1.7.0",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "vue-wasm": "^1.0.0"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.10",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/tsconfig": "^0.5.1",
    "@wasm-tool/rollup-plugin-rust": "^2.4.5",
    "npm-run-all2": "^6.1.1",
    "typescript": "~5.3.0",
    "vite": "^5.1.3",
    "vite-plugin-wasm-pack": "^0.1.12",
    "vue-tsc": "^1.8.27"
  }
}
